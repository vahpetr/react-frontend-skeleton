{
  "name": "frontend-skeleton",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">9.11.1"
  },
  "engineStrict": true,
  "scripts": {
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "test": "node scripts/test.js --verbose --env=jsdom --coverage",
    "update": "ncu -u",
    "ncu": "ncu",
    "reinstall": "rimraf node_modules && npm install",
    "tsc": "tsc",
    "tslint": "tslint --fix -c tslint.json -e **/node_modules/** -e **/build/** **/*.ts*",
    "documentation": "typedoc  --out ./documentation --tsconfig ./tsconfig.json ./src"
  },
  "devDependencies": {
    "@types/node": "9.6.7",
    "npm-check-updates": "2.14.2",
    "rimraf": "2.6.2",
    "webpack-bundle-analyzer": "2.11.1",
    "webpack-dev-server": "2.11.1"
  },
  "dependencies": {
    "@fortawesome/fontawesome": "1.1.5",
    "@fortawesome/fontawesome-free-brands": "5.0.10",
    "@fortawesome/fontawesome-free-regular": "5.0.10",
    "@fortawesome/fontawesome-free-solid": "5.0.10",
    "@types/highcharts": "5.0.22",
    "@types/history": "4.6.2",
    "@types/jest": "22.2.3",
    "@types/qs": "6.5.1",
    "@types/react": "16.3.13",
    "@types/react-dom": "16.0.5",
    "@types/react-redux": "5.0.18",
    "@types/react-router-dom": "4.2.6",
    "@types/react-router-redux": "5.0.13",
    "@types/redux-logger": "3.0.6",
    "app-root-path": "2.0.1",
    "autoprefixer": "8.3.0",
    "axios": "0.18.0",
    "babel-jest": "22.4.3",
    "babel-loader": "7.1.4",
    "babel-preset-react-app": "3.1.1",
    "bootstrap": "4.1.0",
    "case-sensitive-paths-webpack-plugin": "2.1.2",
    "chalk": "2.4.1",
    "css-loader": "0.28.11",
    "dotenv": "5.0.1",
    "duplicate-package-checker-webpack-plugin": "2.1.0",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.11",
    "fork-ts-checker-webpack-plugin": "0.4.1",
    "fs-extra": "5.0.0",
    "highcharts": "6.1.0",
    "history": "4.7.2",
    "html-webpack-plugin": "2.30.1",
    "jest": "22.4.3",
    "less": "3.0.2",
    "less-loader": "4.1.0",
    "monaco-editor": "^0.12.0",
    "node-sass": "4.9.0",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "3.3.0",
    "postcss-loader": "2.1.4",
    "promise": "8.0.1",
    "qs": "6.5.1",
    "raf": "3.4.0",
    "react": "16.3.2",
    "react-dev-utils": "5.0.1",
    "react-dom": "16.3.2",
    "react-hot-loader": "4.1.2",
    "react-redux": "5.0.7",
    "react-router-dom": "4.2.2",
    "react-router-redux": "5.0.0-alpha.9",
    "redux": "3.7.2",
    "redux-devtools-extension": "2.13.2",
    "redux-logger": "3.0.6",
    "redux-saga": "0.16.0",
    "sass-loader": "7.0.1",
    "source-map-loader": "0.2.3",
    "stats-webpack-plugin": "0.6.2",
    "style-loader": "0.21.0",
    "sw-precache-webpack-plugin": "0.11.5",
    "ts-jest": "22.4.4",
    "ts-loader": "3.5.0",
    "tsconfig-paths-webpack-plugin": "3.0.4",
    "tslint": "5.9.1",
    "tslint-loader": "3.6.0",
    "tslint-react": "3.5.1",
    "tslint-sonarts": "1.6.0",
    "typedoc": "0.11.1",
    "typedoc-webpack-plugin": "1.1.4",
    "typescript": "2.8.3",
    "uglify-js": "3.3.22",
    "uglifyjs-webpack-plugin": "1.2.5",
    "url-loader": "1.0.1",
    "webpack": "3.11.0",
    "webpack-manifest-plugin": "1.3.2",
    "whatwg-fetch": "2.0.4"
  },
  "jest": {
    "coverageThreshold": {
      "global": {
        "branches": 0,
        "functions": 0,
        "lines": 0,
        "statements": 0
      }
    },
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "moduleDirectories": [
      "node_modules",
      "."
    ],
    "moduleFileExtensions": [
      "mjs",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "web.js",
      "js",
      "web.jsx",
      "jsx",
      "json",
      "node"
    ],
    "testMatch": [
      "<rootDir>/test/**/__tests__/**/*.(j|t)s?(x)",
      "<rootDir>/test/**/?(*.)(e2e).(j|t)s?(x)",
      "<rootDir>/src/**/__tests__/**/*.(j|t)s?(x)",
      "<rootDir>/src/**/?(*.)(spec).(j|t)s?(x)"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      ".+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      ".+\\.tsx?$": "<rootDir>/config/jest/typescriptTransform.js",
      ".+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs|ts|tsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "globals": {
      "ts-jest": {
        "tsConfigFile": "tsconfig.test.json"
      }
    }
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
